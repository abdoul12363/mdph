{
  "version": 2,
  "builds": [
    {
      "src": "src/pages/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "src/pages/form.html",
      "use": "@vercel/static"
    },
    {
      "src": "src/**/*.{js,css}",
      "use": "@vercel/static"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    },
    {
      "src": "data/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/form",
      "dest": "/src/pages/form.html"
    },
    {
      "src": "/(.*)",
      "dest": "/src/pages/index.html"
    }
  ]
}
